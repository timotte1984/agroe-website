
<div class="devise-form-container">
  <h2>Connexion à l'espace AgroClubRSE</h2>
  <h5>Espace réservé aux adhérents <a href="https://res.cloudinary.com/julienmottet/image/upload/v1559370042/bulletin-adhesion_AgroClub.pdf", class="engage-back">"Agro ClubRSE"</a></h5>
  <div class="devise-form">
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-inputs">
      <%= f.input :email,
                  required: false,
                  autofocus: true,
                  input_html: { autocomplete: "email" } %>
      <%= f.input :password,
                  required: false,
                  input_html: { autocomplete: "current-password" } %>
      <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, "Log in" , class: "btn btn-success"%>
    </div>
  <% end %>
  </div>
</div>



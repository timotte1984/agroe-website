

<div class="section s-white">
  <div id="home-title">
    <span id="home-title-icon"> 🏛</span>
    <br/>
    Pôle des entreprises agroalimentaires <span class="engage"><strong>engagées</strong></span> des Hauts-de-France.
  </div>
  <div class="sections-titles-container">
    <div class="section-title-content">
      <a href="#veille"> 🗞 Veille</a>
    </div>
    <div class="section-title-content">
      <a href="#agroclub">💬 AgroClub</a>
    </div>
    <div class="section-title-content">
      <a href="#visite">🚶 Visites</a>
    </div>
    <div class="section-title-content">
      <a href="#webinaire">💻 Webinaires</a>
    </div>
    <div class="section-title-content">
      <a href="#divers"> 🗄 Divers</a>
    </div>
  </div>
  <span id="veille"></span>
  <div class="title-cards-section">
    Veille Numérique et RSE
  </div>
  <div class="cards-content">
    <div class="row">
    <% @events.where('event_type = ?', 'Veille').order(date: :desc).each do |e| %>
      <%= render 'shared/events/event_card', event: e %>
    <% end %>
    </div>
  </div>
  <span id="agroclub"></span>
  <div class="title-cards-section">
    AgroClubs - comptes-rendus, photos et présentations
  </div>
  <div class="cards-content">
    <div class="row">
    <% @events.where('event_type = ?', 'AgroClub').order(date: :desc).each do |e| %>
      <%= render 'shared/events/event_card', event: e %>
    <% end %>
    </div>
  </div>
  <span id="visite"></span>
  <div class="title-cards-section">
    Visites - comptes-rendus et photos
  </div>
  <div class="cards-content">
    <div class="row">
    <% @events.where('event_type = ?', 'Visite').order(date: :desc).each do |e| %>
      <%= render 'shared/events/event_card', event: e %>
    <% end %>
    </div>
  </div>
  <span id="webinaire"></span>
  <div class="title-cards-section">
    Webinaires - comptes-rendus et accès aux vidéos
  </div>
  <div class="cards-content">
    <div class="row">
    <% @events.where('event_type = ?', 'Webinaire').order(date: :desc).each do |e| %>
      <%= render 'shared/events/event_card', event: e %>
    <% end %>
    </div>
  </div>
  <span id="divers"></span>
  <div class="title-cards-section">
    Divers
  </div>
  <div class="cards-content">
    <div class="row">
    <% @events.where('event_type = ?', '2019').order(date: :desc).each do |e| %>
      <%= render 'shared/events/event_card', event: e %>
    <% end %>
    </div>
  </div>
</div>
